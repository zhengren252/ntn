{
  "dashboard": {
    "id": null,
    "title": "MMS - Market Microstructure Simulation Engine",
    "tags": ["mms", "trading", "simulation", "microstructure"],
    "style": "dark",
    "timezone": "browser",
    "refresh": "30s",
    "schemaVersion": 30,
    "version": 1,
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": ["5s", "10s", "30s", "1m", "5m", "15m", "30m", "1h", "2h", "1d"]
    },
    "templating": {
      "list": [
        {
          "name": "instance",
          "type": "query",
          "query": "label_values(up{job=~\"mms-.*\"}, instance)",
          "refresh": 1,
          "includeAll": true,
          "allValue": ".*",
          "multi": true
        },
        {
          "name": "job",
          "type": "query",
          "query": "label_values(up{job=~\"mms-.*\"}, job)",
          "refresh": 1,
          "includeAll": true,
          "allValue": ".*",
          "multi": true
        }
      ]
    },
    "panels": [
      {
        "id": 1,
        "title": "Service Status Overview",
        "type": "stat",
        "gridPos": {"h": 4, "w": 6, "x": 0, "y": 0},
        "targets": [
          {
            "expr": "up{job=~\"mms-.*\"}",
            "legendFormat": "{{job}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "thresholds"},
            "thresholds": {
              "steps": [
                {"color": "red", "value": 0},
                {"color": "green", "value": 1}
              ]
            },
            "mappings": [
              {"options": {"0": {"text": "DOWN"}, "1": {"text": "UP"}}}
            ]
          }
        }
      },
      {
        "id": 2,
        "title": "Request Rate",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 6, "y": 0},
        "targets": [
          {
            "expr": "rate(http_requests_total{job=~\"$job\", instance=~\"$instance\"}[5m])",
            "legendFormat": "{{job}} - {{method}} {{status}}",
            "refId": "A"
          }
        ],
        "yAxes": [
          {"label": "Requests/sec", "min": 0}
        ]
      },
      {
        "id": 3,
        "title": "Response Time",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 18, "y": 0},
        "targets": [
          {
            "expr": "histogram_quantile(0.50, rate(http_request_duration_seconds_bucket{job=~\"$job\", instance=~\"$instance\"}[5m]))",
            "legendFormat": "50th percentile",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.95, rate(http_request_duration_seconds_bucket{job=~\"$job\", instance=~\"$instance\"}[5m]))",
            "legendFormat": "95th percentile",
            "refId": "B"
          },
          {
            "expr": "histogram_quantile(0.99, rate(http_request_duration_seconds_bucket{job=~\"$job\", instance=~\"$instance\"}[5m]))",
            "legendFormat": "99th percentile",
            "refId": "C"
          }
        ],
        "yAxes": [
          {"label": "Seconds", "min": 0}
        ]
      },
      {
        "id": 4,
        "title": "System Resources",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8},
        "targets": [
          {
            "expr": "100 - (avg by(instance) (irate(node_cpu_seconds_total{mode=\"idle\", instance=~\"$instance\"}[5m])) * 100)",
            "legendFormat": "CPU Usage %",
            "refId": "A"
          },
          {
            "expr": "(1 - (node_memory_MemAvailable_bytes{instance=~\"$instance\"} / node_memory_MemTotal_bytes{instance=~\"$instance\"})) * 100",
            "legendFormat": "Memory Usage %",
            "refId": "B"
          },
          {
            "expr": "(1 - (node_filesystem_avail_bytes{instance=~\"$instance\"} / node_filesystem_size_bytes{instance=~\"$instance\"})) * 100",
            "legendFormat": "Disk Usage %",
            "refId": "C"
          }
        ],
        "yAxes": [
          {"label": "Percentage", "min": 0, "max": 100}
        ]
      },
      {
        "id": 5,
        "title": "Simulation Metrics",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8},
        "targets": [
          {
            "expr": "rate(simulation_tasks_total{instance=~\"$instance\"}[5m])",
            "legendFormat": "Tasks/sec - {{status}}",
            "refId": "A"
          },
          {
            "expr": "simulation_queue_size{instance=~\"$instance\"}",
            "legendFormat": "Queue Size",
            "refId": "B"
          }
        ],
        "yAxes": [
          {"label": "Count", "min": 0}
        ]
      },
      {
        "id": 6,
        "title": "Simulation Execution Time",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 16},
        "targets": [
          {
            "expr": "histogram_quantile(0.50, rate(simulation_execution_duration_seconds_bucket{instance=~\"$instance\"}[5m]))",
            "legendFormat": "50th percentile",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.95, rate(simulation_execution_duration_seconds_bucket{instance=~\"$instance\"}[5m]))",
            "legendFormat": "95th percentile",
            "refId": "B"
          },
          {
            "expr": "avg(rate(simulation_execution_duration_seconds_sum{instance=~\"$instance\"}[5m]) / rate(simulation_execution_duration_seconds_count{instance=~\"$instance\"}[5m]))",
            "legendFormat": "Average",
            "refId": "C"
          }
        ],
        "yAxes": [
          {"label": "Seconds", "min": 0}
        ]
      },
      {
        "id": 7,
        "title": "Redis Metrics",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 16},
        "targets": [
          {
            "expr": "redis_connected_clients",
            "legendFormat": "Connected Clients",
            "refId": "A"
          },
          {
            "expr": "rate(redis_commands_processed_total[5m])",
            "legendFormat": "Commands/sec",
            "refId": "B"
          },
          {
            "expr": "redis_memory_used_bytes / 1024 / 1024",
            "legendFormat": "Memory Used (MB)",
            "refId": "C"
          }
        ],
        "yAxes": [
          {"label": "Count/MB", "min": 0}
        ]
      },
      {
        "id": 8,
        "title": "Cache Performance",
        "type": "stat",
        "gridPos": {"h": 4, "w": 6, "x": 0, "y": 24},
        "targets": [
          {
            "expr": "rate(cache_hits_total[5m]) / (rate(cache_hits_total[5m]) + rate(cache_misses_total[5m])) * 100",
            "legendFormat": "Hit Rate %",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "thresholds"},
            "thresholds": {
              "steps": [
                {"color": "red", "value": 0},
                {"color": "yellow", "value": 70},
                {"color": "green", "value": 90}
              ]
            },
            "unit": "percent"
          }
        }
      },
      {
        "id": 9,
        "title": "Error Rate",
        "type": "stat",
        "gridPos": {"h": 4, "w": 6, "x": 6, "y": 24},
        "targets": [
          {
            "expr": "rate(http_requests_total{status=~\"5..\", job=~\"$job\", instance=~\"$instance\"}[5m]) / rate(http_requests_total{job=~\"$job\", instance=~\"$instance\"}[5m]) * 100",
            "legendFormat": "Error Rate %",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "thresholds"},
            "thresholds": {
              "steps": [
                {"color": "green", "value": 0},
                {"color": "yellow", "value": 1},
                {"color": "red", "value": 5}
              ]
            },
            "unit": "percent"
          }
        }
      },
      {
        "id": 10,
        "title": "ZeroMQ Queue Status",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 24},
        "targets": [
          {
            "expr": "zmq_queue_size{instance=~\"$instance\"}",
            "legendFormat": "Queue Size - {{queue}}",
            "refId": "A"
          },
          {
            "expr": "rate(zmq_messages_sent_total{instance=~\"$instance\"}[5m])",
            "legendFormat": "Messages Sent/sec",
            "refId": "B"
          },
          {
            "expr": "rate(zmq_messages_received_total{instance=~\"$instance\"}[5m])",
            "legendFormat": "Messages Received/sec",
            "refId": "C"
          }
        ],
        "yAxes": [
          {"label": "Count/Rate", "min": 0}
        ]
      },
      {
        "id": 11,
        "title": "Database Performance",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 32},
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(database_query_duration_seconds_bucket{instance=~\"$instance\"}[5m]))",
            "legendFormat": "95th percentile query time",
            "refId": "A"
          },
          {
            "expr": "rate(database_queries_total{instance=~\"$instance\"}[5m])",
            "legendFormat": "Queries/sec",
            "refId": "B"
          },
          {
            "expr": "database_connections_active{instance=~\"$instance\"}",
            "legendFormat": "Active Connections",
            "refId": "C"
          }
        ],
        "yAxes": [
          {"label": "Seconds/Count", "min": 0}
        ]
      },
      {
        "id": 12,
        "title": "Business Metrics",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 32},
        "targets": [
          {
            "expr": "avg(simulation_slippage{instance=~\"$instance\"})",
            "legendFormat": "Average Slippage",
            "refId": "A"
          },
          {
            "expr": "avg(simulation_fill_probability{instance=~\"$instance\"})",
            "legendFormat": "Average Fill Probability",
            "refId": "B"
          },
          {
            "expr": "avg(simulation_price_impact{instance=~\"$instance\"})",
            "legendFormat": "Average Price Impact",
            "refId": "C"
          }
        ],
        "yAxes": [
          {"label": "Value", "min": 0}
        ]
      },
      {
        "id": 13,
        "title": "Top Simulation Scenarios",
        "type": "piechart",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 40},
        "targets": [
          {
            "expr": "sum by (scenario) (rate(simulation_tasks_total{status=\"completed\", instance=~\"$instance\"}[1h]))",
            "legendFormat": "{{scenario}}",
            "refId": "A"
          }
        ]
      },
      {
        "id": 14,
        "title": "Alert Status",
        "type": "table",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 40},
        "targets": [
          {
            "expr": "ALERTS{job=~\"$job\", instance=~\"$instance\"}",
            "format": "table",
            "refId": "A"
          }
        ],
        "transformations": [
          {
            "id": "organize",
            "options": {
              "excludeByName": {},
              "indexByName": {},
              "renameByName": {
                "alertname": "Alert",
                "alertstate": "State",
                "severity": "Severity"
              }
            }
          }
        ]
      }
    ],
    "annotations": {
      "list": [
        {
          "name": "Deployments",
          "datasource": "Prometheus",
          "enable": true,
          "expr": "changes(process_start_time_seconds{job=~\"mms-.*\"}[1m]) > 0",
          "iconColor": "blue",
          "titleFormat": "Deployment",
          "textFormat": "{{job}} restarted"
        }
      ]
    }
  },
  "overwrite": true
}