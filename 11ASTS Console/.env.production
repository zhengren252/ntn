# 生产环境配置
NODE_ENV=production
APP_ENV=production

# API配置
VITE_API_BASE_URL=http://api_factory:8080
VITE_WS_URL=ws://api_factory:8080/ws

# Redis配置
VITE_REDIS_URL=redis://redis:6379

# 应用配置
VITE_APP_NAME=ASTS Console
VITE_APP_VERSION=1.0.0
VITE_APP_DESCRIPTION=NeuroTrade Nexus 前端管理界面

# 安全配置
VITE_ENABLE_HTTPS=false
VITE_CORS_ORIGIN=*

# 性能配置
VITE_ENABLE_PWA=true
VITE_ENABLE_GZIP=true
VITE_BUNDLE_ANALYZER=false

# 日志配置
VITE_LOG_LEVEL=warn
VITE_ENABLE_CONSOLE_LOG=false

# 监控配置
VITE_ENABLE_SENTRY=false
VITE_ENABLE_ANALYTICS=false

# 功能开关
VITE_ENABLE_MOCK_DATA=false
VITE_ENABLE_DEBUG_MODE=false
VITE_ENABLE_DEV_TOOLS=false