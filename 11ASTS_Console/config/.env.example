# 环境变量配置示例
# 复制此文件为.env并填入实际值

# 应用配置
APP_NAME="NTN Module"
DEBUG=False
HOST=0.0.0.0
PORT=8000

# ZeroMQ配置
ZMQ_PORT=5555

# 数据库配置
DATABASE_URL=postgresql://user:password@localhost/dbname

# Redis配置
REDIS_URL=redis://localhost:6379

# 日志配置
LOG_LEVEL=INFO

# API配置
API_KEY=your_api_key_here
API_SECRET=your_api_secret_here
