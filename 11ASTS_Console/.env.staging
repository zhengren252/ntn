# 测试环境配置
NODE_ENV=staging
APP_ENV=staging

# API配置
VITE_API_BASE_URL=http://api_factory_staging:8080
VITE_WS_URL=ws://api_factory_staging:8080/ws

# Redis配置
VITE_REDIS_URL=redis://redis_staging:6379

# 应用配置
VITE_APP_NAME=ASTS Console (Staging)
VITE_APP_VERSION=1.0.0-staging
VITE_APP_DESCRIPTION=NeuroTrade Nexus 前端管理界面 - 测试环境

# 安全配置
VITE_ENABLE_HTTPS=false
VITE_CORS_ORIGIN=*

# 性能配置
VITE_ENABLE_PWA=false
VITE_ENABLE_GZIP=true
VITE_BUNDLE_ANALYZER=true

# 日志配置
VITE_LOG_LEVEL=info
VITE_ENABLE_CONSOLE_LOG=true

# 监控配置
VITE_ENABLE_SENTRY=true
VITE_ENABLE_ANALYTICS=false

# 功能开关
VITE_ENABLE_MOCK_DATA=false
VITE_ENABLE_DEBUG_MODE=true
VITE_ENABLE_DEV_TOOLS=true

# 测试配置
VITE_ENABLE_E2E_TESTS=true
VITE_TEST