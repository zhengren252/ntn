# 开发环境配置
NODE_ENV=development
APP_ENV=development

# API配置
VITE_API_BASE_URL=http://localhost:8080
VITE_WS_URL=ws://localhost:8080/ws

# Redis配置
VITE_REDIS_URL=redis://localhost:6379

# 应用配置
VITE_APP_NAME=ASTS Console (Dev)
VITE_APP_VERSION=1.0.0-dev
VITE_APP_DESCRIPTION=NeuroTrade Nexus 前端管理界面 - 开发环境

# 安全配置
VITE_ENABLE_HTTPS=false
VITE_CORS_ORIGIN=*

# 性能配置
VITE_ENABLE_PWA=false
VITE_ENABLE_GZIP=false
VITE_BUNDLE_ANALYZER=true

# 日志配置
VITE_LOG_LEVEL=debug
VITE_ENABLE_CONSOLE_LOG=true

# 监控配置
VITE_ENABLE_SENTRY=false
VITE_ENABLE_ANALYTICS=false

# 功能开关
VITE_ENABLE_MOCK_DATA=true
VITE_ENABLE_DEBUG_MODE=true
VITE_ENABLE_DEV_TOOLS=true

# 开发配置
VITE_HOT_RELOAD=true
VITE_OPEN_BROWSER=true
VITE_DEV_PORT=3000

# 测试配置
VITE_ENABLE_E2E_TESTS=true
VITE_TEST_TIMEOUT=10000
VITE_COVERAGE_THRESHOLD=80